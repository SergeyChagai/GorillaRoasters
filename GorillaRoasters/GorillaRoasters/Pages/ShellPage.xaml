<?xml version="1.0" encoding="utf-8" ?>
<Shell
    x:Class="GorillaRoasters.ShellPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:GorillaRoasters">

    <FlyoutItem Title="CoffeShop" Route="coffeShop">
        <Tab>
            <ShellContent>
                <pages:MainPage />
            </ShellContent>
        </Tab>

    </FlyoutItem>
    <FlyoutItem Title="StarWarsInfo" Route="starWarsPage">
        <Tab>
            <ShellContent>
                <pages:StarWarsPage />
            </ShellContent>
        </Tab>

    </FlyoutItem>
</Shell>